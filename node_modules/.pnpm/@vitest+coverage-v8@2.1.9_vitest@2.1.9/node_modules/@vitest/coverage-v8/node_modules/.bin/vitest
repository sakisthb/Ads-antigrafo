#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/airathanasios/Claude-Cursor-ai/node_modules/.pnpm/vitest@2.1.9_@types+node@22.15.21_@vitest+ui@2.1.9_jsdom@25.0.1_lightningcss@1.30.1/node_modules/vitest/node_modules:/Users/airathanasios/Claude-Cursor-ai/node_modules/.pnpm/vitest@2.1.9_@types+node@22.15.21_@vitest+ui@2.1.9_jsdom@25.0.1_lightningcss@1.30.1/node_modules:/Users/airathanasios/Claude-Cursor-ai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/airathanasios/Claude-Cursor-ai/node_modules/.pnpm/vitest@2.1.9_@types+node@22.15.21_@vitest+ui@2.1.9_jsdom@25.0.1_lightningcss@1.30.1/node_modules/vitest/node_modules:/Users/airathanasios/Claude-Cursor-ai/node_modules/.pnpm/vitest@2.1.9_@types+node@22.15.21_@vitest+ui@2.1.9_jsdom@25.0.1_lightningcss@1.30.1/node_modules:/Users/airathanasios/Claude-Cursor-ai/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@2.1.9_@types+node@22.15.21_@vitest+ui@2.1.9_jsdom@25.0.1_lightningcss@1.30.1/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@2.1.9_@types+node@22.15.21_@vitest+ui@2.1.9_jsdom@25.0.1_lightningcss@1.30.1/node_modules/vitest/vitest.mjs" "$@"
fi
