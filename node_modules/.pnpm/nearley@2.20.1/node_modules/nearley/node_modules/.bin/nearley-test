#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/airathanasios/volo-chat/node_modules/.pnpm/nearley@2.20.1/node_modules/nearley/bin/node_modules:/Users/airathanasios/volo-chat/node_modules/.pnpm/nearley@2.20.1/node_modules/nearley/node_modules:/Users/airathanasios/volo-chat/node_modules/.pnpm/nearley@2.20.1/node_modules:/Users/airathanasios/volo-chat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/airathanasios/volo-chat/node_modules/.pnpm/nearley@2.20.1/node_modules/nearley/bin/node_modules:/Users/airathanasios/volo-chat/node_modules/.pnpm/nearley@2.20.1/node_modules/nearley/node_modules:/Users/airathanasios/volo-chat/node_modules/.pnpm/nearley@2.20.1/node_modules:/Users/airathanasios/volo-chat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nearley-test.js" "$@"
else
  exec node  "$basedir/../../bin/nearley-test.js" "$@"
fi
